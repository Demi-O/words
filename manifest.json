{
  "manifest_version": 2,

  "name": "Words",
  "description": "This extension allows users to access dictionary content of a word",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}